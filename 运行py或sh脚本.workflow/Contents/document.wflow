					<string>#!/bin/bash

# 获取文件扩展名
ext="${1##*.}"

# 判断文件类型并运行相应的解释器
if [[ "$ext" == "sh" ]]; then
    /bin/sh "$1"
elif [[ "$ext" == "py" ]]; then
    /opt/homebrew/bin/python3 "$1"
else
    echo "Unsupported file type: $1"
    exit 1
fi</string>
