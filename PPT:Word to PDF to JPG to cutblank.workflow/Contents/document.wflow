					<string>on run {inputPath}	try		-- 检查输入是否为列表并提取路径		if class of inputPath is list and (count of inputPath) &gt; 0 then			set pathItem to item 1 of inputPath			if class of pathItem is alias then				set posixPath to POSIX path of pathItem			else				error "列表中的项目不是有效的 alias 类型。"			end if		else			error "输入不是有效的列表或为空。"		end if				-- 检查路径末尾是否为 '1/'，并替换为 '2/'		if posixPath ends with "1/" then			set posixPath to text 1 thru -3 of posixPath &amp; "2/"		else			error "路径末尾不是 '1/'，无法替换为 '2/'。"		end if				-- 转换为 Finder 可用的 HFS 路径		set hfsPath to POSIX file posixPath				-- 打开 Finder 并设置路径		tell application "Finder"			activate			set currentWindow to Finder window 1			set target of currentWindow to hfsPath			set currentView to column view -- 强制切换到列视图			delay 0.5 -- 等待 Finder 加载完成		end tell				-- 使用系统事件触发快捷键		tell application "System Events"			tell process "Finder"				keystroke "o" using {command down, option down} -- 快捷键：Command + Option + o			end tell		end tell			on error errMsg		-- 捕获错误并提示		display dialog "发生错误：" &amp; errMsg buttons {"OK"} default button 1	end tryend run</string>
