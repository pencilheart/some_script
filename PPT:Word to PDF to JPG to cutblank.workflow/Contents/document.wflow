# 需要安装依赖：pip3 install pillow numpy
from concurrent.futures import ThreadPoolExecutor
from PIL import Image
import numpy as np
import os
import sys

def crop_white(image_path, output_path, threshold=240, output_format="TIFF", resize_factor=0.5):
    img = Image.open(image_path).convert("RGB")
    img_np = np.array(img)
    mask = np.all(img_np &lt; threshold, axis=-1)
    coords = np.argwhere(mask)

    if coords.size &gt; 0:
        y_min, x_min = coords.min(axis=0)
        y_max, x_max = coords.max(axis=0) + 1
        cropped_img = img.crop((x_min, y_min, x_max, y_max))
        
        # 降低分辨率
        new_width = int(cropped_img.width * resize_factor)
        new_height = int(cropped_img.height * resize_factor)
        resized_img = cropped_img.resize((new_width, new_height), Image.Resampling.LANCZOS)

        # 保存为指定格式
        resized_img.save(output_path, format=output_format)

def batch_crop_images(input_folder, output_folder, threshold=240, max_workers=4, output_format="TIFF", resize_factor=0.5):
    if not os.path.exists(output_folder):
        os.makedirs(output_folder)

    files = sorted(f for f in os.listdir(input_folder) if f.endswith((".jpg", ".jpeg")))
    tasks = []

    with ThreadPoolExecutor(max_workers=max_workers) as executor:
        for filename in files:
            input_path = os.path.join(input_folder, filename)
            output_filename = os.path.splitext(filename)[0] + (".tiff" if output_format == "TIFF" else ".jpeg")
            output_path = os.path.join(output_folder, output_filename)
            tasks.append(executor.submit(crop_white, input_path, output_path, threshold, output_format, resize_factor))

        for future in tasks:
            future.result()  # 等待所有任务完成

if __name__ == "__main__":
    current_dir = os.path.dirname(os.path.abspath(__file__))
    input_folder = os.path.join(current_dir, '1')
    output_folder = os.path.join(current_dir, '2')

    # 从命令行参数获取输出格式
    output_format = sys.argv[1].upper() if len(sys.argv) &gt; 1 else "TIFF"
    resize_factor = float(sys.argv[2]) if len(sys.argv) &gt; 2 else 0.5
    batch_crop_images(input_folder, output_folder, max_workers=8, output_format=output_format, resize_factor=resize_factor)
EOF

