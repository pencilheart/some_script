filePath="$1"
folderPath=$(dirname "$filePath")
parentFolderPath=$(dirname "$folderPath")

# Ask user for output format using AppleScript
outputFormat=$(osascript -e 'tell application "System Events" to display dialog "Choose the output format:" buttons {"TIFF", "JPEG"} default button "TIFF" with icon 1' \
    -e 'button returned of result')

# Ask user for resize factor using AppleScript
resizeFactor=$(osascript -e 'tell application "System Events" to display dialog "Enter the resize factor (e.g., 0.5 for half resolution):" default answer "1" with icon 1' \
    -e 'text returned of result')

# Check if user canceled or input is invalid for output format
if [[ "$outputFormat" != "TIFF" &amp;&amp; "$outputFormat" != "JPEG" ]]; then
    echo "Invalid choice or user canceled. Exiting..."
    exit 1
fi

# Validate resize factor
if ! [[ "$resizeFactor" =~ ^[0-9]+(\.[0-9]+)?$ ]]; then
    echo "Invalid resize factor. Exiting..."
    exit 1
fi

# Create the jpg_cut.py file in the parent folder
