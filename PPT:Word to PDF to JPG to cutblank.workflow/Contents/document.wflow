				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>运行AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}	set theOutput to {}	set targetFolders to {} -- 用于记录所有创建的目标文件夹		-- 检查并处理目标文件夹（pptword2pdfcut）	set parentDir to POSIX path of (do shell script "dirname " &amp; quoted form of (POSIX path of (item 1 of input)))	set targetDir to parentDir &amp; "/pptword2pdfcut/1"	try		-- 如果文件夹存在，删除它		if (do shell script "test -d " &amp; quoted form of targetDir &amp; " &amp;&amp; echo true || echo false") is "true" then			do shell script "rm -rf " &amp; quoted form of targetDir		end if		-- 创建新的目标文件夹		do shell script "mkdir -p " &amp; quoted form of targetDir	on error errMsg		display dialog "Failed to prepare target folder. Error: " &amp; errMsg		return	end try		-- 循环处理输入文件	repeat with i in input		set t to i as string		set fileExtension to do shell script "basename " &amp; quoted form of (POSIX path of i) &amp; " | sed 's/.*\\.//'"				-- 处理 Word 和 PowerPoint 文件		if fileExtension is in {"doc", "docx"} then			-- 如果是 Word 文件			set pdfPath to my makeNewPath(i, "word", targetDir)			tell application "Microsoft Word"				open i				set activeDoc to active document				-- 使用正确的保存命令				save as activeDoc file name (POSIX file pdfPath) file format format PDF				close active document saving no			end tell		else if fileExtension is in {"ppt", "pptx"} then			-- 如果是 PowerPoint 文件			set pdfPath to my makeNewPath(i, "ppt", targetDir)			tell application "Microsoft PowerPoint"				open i				set activePres to active presentation				-- 使用正确的保存命令				save activePres in (POSIX file pdfPath) as save as PDF				close active presentation saving no			end tell		end if				-- 将 PDF 路径添加到输出列表		set end of theOutput to pdfPath	end repeat		-- 返回所有目标文件夹的 Finder 项	set finderItems to {}	set folderAlias to POSIX file targetDir as alias	set end of finderItems to folderAlias		return finderItemsend runon makeNewPath(f, type, targetDir)	-- 获取输入文件的基本文件名	set fileName to do shell script "basename " &amp; quoted form of (POSIX path of f)		-- 根据文件类型选择正确的 PDF 路径	if type is "word" then		-- 处理 Word 文件		if f ends with ".docx" then			return targetDir &amp; "/" &amp; (do shell script "basename -s .docx " &amp; quoted form of (POSIX path of f)) &amp; ".pdf"		else			return targetDir &amp; "/" &amp; (do shell script "basename -s .doc " &amp; quoted form of (POSIX path of f)) &amp; ".pdf"		end if	else if type is "ppt" then		-- 处理 PowerPoint 文件		if f ends with ".pptx" then			return targetDir &amp; "/" &amp; (do shell script "basename -s .pptx " &amp; quoted form of (POSIX path of f)) &amp; ".pdf"		else			return targetDir &amp; "/" &amp; (do shell script "basename -s .ppt " &amp; quoted form of (POSIX path of f)) &amp; ".pdf"		end if	end ifend makeNewPath</string>
